  "exports": {
    ".": "./lib/index.js",
    "./lib/style.css": "./lib/style.css"
  },
  "types": "./lib/index.d.ts",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "test": "vitest",
